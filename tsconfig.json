{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@data/*": [
        "data/*"
      ],
      "@features/*": [
        "features/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@models/*": [
        "models/*"
      ]
    },
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}