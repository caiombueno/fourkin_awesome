{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": [
      "@types/jest"
    ],
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@data": [
        "data"
      ],
      "data/*": [
        "data/*"
      ],
      "@features": [
        "features"
      ],
      "@features/*": [
        "features/*"
      ],
      "@components": [
        "components"
      ],
      "@navigation": [
        "navigation"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@models": [
        "models"
      ],
      "@redux": [
        "redux"
      ],
      "@firebaseConfig": [
        "../firebaseConfig.js"
      ],
    },
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}